{
  "$schema": "https://json.schemastore.org/tsconfig",

  "extends": "../base/tsconfig.json",

  "display": "React",

  "compilerOptions": {
    /* Modules */
    "module": "esnext",

    /* Emit */
    "noEmit": true,
    "importsNotUsedAsValues": "error",

    /* Interop Constraints */
    "isolatedModules": true,

    /* Language and Environment */
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "es2015"
  }
}
